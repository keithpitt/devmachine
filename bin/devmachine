#!/usr/bin/env bash

bin_name="${0##*/}"

case "$1" in
  shellenv)
    source "$DEVMACHINE_PATH/cli/$bin_name/shellenv.sh" "${@:2}"
    ;;

  cachebust)
    source "$DEVMACHINE_PATH/cli/$bin_name/cachebust.sh" "${@:2}"
    ;;

  edit)
    source "$DEVMACHINE_PATH/cli/$bin_name/edit.sh" "${@:2}"
    ;;

  *)
    source "$DEVMACHINE_PATH/cli/$bin_name/tools.sh" "${@:2}"
    ;;
esac
